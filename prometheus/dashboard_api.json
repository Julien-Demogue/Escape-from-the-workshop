{
  "title": "Node.js API Prometheus Metrics",
  "tags": ["nodejs", "prometheus", "api"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "RequÃªtes HTTP (par route)",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m])) by (route)",
          "legendFormat": "{{ route }}",
          "datasource": "Prometheus"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Latence moyenne (s)",
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[1m]) / rate(http_request_duration_seconds_count[1m])",
          "legendFormat": "latence moyenne",
          "datasource": "Prometheus"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "P95 Latence (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "P95",
          "datasource": "Prometheus"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Erreurs HTTP (4xx/5xx)",
      "targets": [
        {
          "expr": "sum(rate(http_errors_total[1m])) by (status)",
          "legendFormat": "{{ status }}",
          "datasource": "Prometheus"
        }
      ],
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 }
    }
  ],
  "templating": {
    "list": []
  }
}